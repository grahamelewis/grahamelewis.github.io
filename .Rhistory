main_DF %>% ggplot(aes(x = judgement_rate, y = vac_rate)) +
geom_point(alpha = 0.7) +
geom_smooth(method = "lm", fill = "gray") +
labs(title = "Eviction Rate vs. Rental Vacancy Rate",
x = "Eviction Rate",
y = "Rental Vacancy Rate") +
theme_bw()
